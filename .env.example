# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# Never commit .env files to version control

# Google Drive API Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# Application Configuration
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

# Security Configuration
VITE_ENABLE_DEBUG_LOGGING=false
VITE_ENABLE_SOURCE_MAPS=false

# Note: Only VITE_ prefixed variables are exposed to the client
# Keep sensitive server-side secrets without VITE_ prefix
# GOOGLE_CLIENT_SECRET=your_secret_here (server-side only)

# Database Configuration (if using server-side database)
# DATABASE_URL=postgresql://username:password@localhost:5432/finance_db
# DATABASE_ENCRYPTION_KEY=your_database_encryption_key_here

# Session Configuration
# SESSION_SECRET=your_session_secret_here
# JWT_SECRET=your_jwt_secret_here